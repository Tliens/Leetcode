package top.tlien.www;

public class leetcode_876_链表的中间结点 {
	public ListNode middleNode(ListNode head) {
        if (head.next == null){
            return head;
        }
        ListNode slow = head;
        ListNode fast = head.next;
        while (fast.next != null && fast.next.next != null){
            slow = slow.next;
            fast = fast.next.next;
        }
        return slow.next;
    }
}
